{
    "name": "uniplaces/request-logger-bundle",
    "description": "Symfony bundle to log information about the request like method, uri, latency",
    "type": "bundle",
    "require": {
        "symfony/http-kernel": "^4.0",
        "psr/log": "^1.0",
        "symfony/config": "^4.0",
        "symfony/dependency-injection": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1"
    },
    "autoload": {
        "psr-4": { "Uniplaces\\RequestLoggerBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Uniplaces\\RequestLoggerBundle\\Tests\\": "/Tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Uniplaces",
            "email": "eng@uniplaces.com"
        }
    ],
    "minimum-stability": "stable"
}
